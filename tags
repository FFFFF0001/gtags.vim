!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
. Installation	README.md	/^## Installation$/;"	h
.gtags.vim	README.md	/^# gtags.vim$/;"	h
AddLib	autoload/gtags.vim	/^function! s:AddLib(path)$/;"	f
Cursor	autoload/gtags.vim	/^function! s:Cursor()$/;"	f
Error	autoload/gtags.vim	/^function! s:Error(msg)$/;"	f
ExecLoad	autoload/gtags.vim	/^function! s:ExecLoad(option, long_option, pattern)$/;"	f
Extract	autoload/gtags.vim	/^function! s:Extract(line, target)$/;"	f
Func	autoload/gtags.vim	/^function! s:Func(type, pattern)$/;"	f
Gozilla	autoload/gtags.vim	/^command! -nargs=0 Gozilla call s:Gozilla()$/;"	c
Gozilla	autoload/gtags.vim	/^function! s:Gozilla()$/;"	f
Gtags	autoload/gtags.vim	/^command! -nargs=* -complete=custom,GtagsCandidate Gtags call s:RunGlobal(<q-args>)$/;"	c
GtagsAddLib	autoload/gtags.vim	/^command! -nargs=+ -complete=dir GtagsAddLib call s:AddLib(<q-args>)$/;"	c
GtagsCandidate	autoload/gtags.vim	/^function! GtagsCandidate(lead, line, pos)$/;"	f
GtagsCandidateCore	autoload/gtags.vim	/^function! GtagsCandidateCore(lead, line, pos)$/;"	f
GtagsCursor	autoload/gtags.vim	/^command! -nargs=0 GtagsCursor call s:Cursor()$/;"	c
GtagsFunc	autoload/gtags.vim	/^command! -nargs=+ -complete=custom,GtagsCandidate GtagsFunc call s:Func(<f-args>)$/;"	c
GtagsMapKeys	autoload/gtags.vim	/^command! GtagsMapKeys call s:MapKeys()$/;"	c
GtagsRemind	autoload/gtags.vim	/^command! -nargs=0 GtagsRemind call s:Remind()$/;"	c
GtagsShowLibPath	autoload/gtags.vim	/^command! -nargs=0 GtagsShowLibPath call s:ShowLibPath()$/;"	c
Gtags_Auto_Map	autoload/gtags.vim	/^    let Gtags_Auto_Map = 0$/;"	v
Gtags_Auto_Map	plugin/gtags.vim	/^ let Gtags_Auto_Map = 0$/;"	v
MapKeys	autoload/gtags.vim	/^function! s:MapKeys()$/;"	f
Memorize	autoload/gtags.vim	/^function! s:Memorize()$/;"	f
Remind	autoload/gtags.vim	/^function! s:Remind()$/;"	f
RunGlobal	autoload/gtags.vim	/^function! s:RunGlobal(line)$/;"	f
ShowLibPath	autoload/gtags.vim	/^function! s:ShowLibPath()$/;"	f
Stack	autoload/gtags.vim	/^function! s:Stack()$/;"	f
TrimOption	autoload/gtags.vim	/^function! s:TrimOption(option)$/;"	f
UpdateDatabase	autoload/gtags.vim	/^function! s:UpdateDatabase()$/;"	f
g:Gtags_Auto_Map	autoload/gtags.vim	/^    let g:Gtags_Auto_Map = 1$/;"	v
g:Gtags_Double_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Double_Quote_Char = '"'$/;"	v
g:Gtags_Double_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Double_Quote_Char = '\\"'$/;"	v
g:Gtags_Efm	autoload/gtags.vim	/^    let g:Gtags_Efm = "%*\\\\S%*\\\\s%l%\\\\s%f%\\\\s%m"$/;"	v
g:Gtags_Efm	autoload/gtags.vim	/^    let g:Gtags_Efm = "%m\\t%f\\t%l"$/;"	v
g:Gtags_OpenQuickfixWindow	autoload/gtags.vim	/^    let g:Gtags_OpenQuickfixWindow = 1$/;"	v
g:Gtags_Result	autoload/gtags.vim	/^    let g:Gtags_Result = "ctags"$/;"	v
g:Gtags_Result	autoload/gtags.vim	/^    let g:Gtags_Result = "ctags-x"$/;"	v
g:Gtags_Shell_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Shell_Quote_Char = "'"$/;"	v
g:Gtags_Shell_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Shell_Quote_Char = '"'$/;"	v
g:Gtags_Single_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Single_Quote_Char = "'"$/;"	v
g:Gtags_Single_Quote_Char	autoload/gtags.vim	/^        let g:Gtags_Single_Quote_Char = s:sq . s:dq . s:sq . s:dq . s:sq$/;"	v
g:Gtags_VerticalWindow	autoload/gtags.vim	/^    let g:Gtags_VerticalWindow = 0$/;"	v
loaded_gtags	autoload/gtags.vim	/^let loaded_gtags = 1$/;"	v
s:crumbs	autoload/gtags.vim	/^    let s:crumbs = s:Stack()$/;"	v
s:dq	autoload/gtags.vim	/^        let s:dq = '"'$/;"	v
s:global_command	autoload/gtags.vim	/^    let s:global_command = "global"$/;"	v
s:global_command	autoload/gtags.vim	/^let s:global_command = $GTAGSGLOBAL$/;"	v
s:sq	autoload/gtags.vim	/^        let s:sq = "'"$/;"	v
s:version	autoload/gtags.vim	/^    let s:version = split(matchstr(ver_str, '[0-9]\\+\\.[0-9]\\+'), '\\.')$/;"	v
ver_str	autoload/gtags.vim	/^    let ver_str = split(system('gtags --version'), '\\n')[0]$/;"	v
